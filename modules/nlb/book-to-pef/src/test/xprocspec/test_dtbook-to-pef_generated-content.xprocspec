    <x:scenario label="basic test of complete titlepage">
        <x:call step="nlb:dtbook-to-pef">
            <x:input port="source">
                <x:document type="inline" xml:base="file:/tmp/book.xml">
                    <dtbook xmlns="http://www.daisy.org/z3986/2005/dtbook/" xml:lang="no" version="2005-3">
                        <head>
                            <meta name="dtb:uid" content="uid"/>
                            <meta name="dc:Creator" content="author"/>
                            <meta name="dc:Title" content="title"/>
                        </head>
                        <book>
                            <bodymatter>
                                <level1>
                                    <h1>bodymatter</h1>
                                </level1>
                            </bodymatter>
                        </book>
                    </dtbook>
                </x:document>
            </x:input>
            <x:option name="pef-output-dir" select="resolve-uri('basic-titlepage/1/pef-output-dir',$temp-dir)"/>
            <x:option name="preview-output-dir" select="resolve-uri('basic-titlepage/1/preview-output-dir',$temp-dir)"/>
            <x:option name="temp-dir" select="resolve-uri('basic-titlepage/1/temp-dir',$temp-dir)"/>
