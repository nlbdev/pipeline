<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog" xmlns:px="http://www.daisy.org/ns/pipeline">
  
  <!-- abstract script (common options and documentations) -->
  <uri name="http://www.nlb.no/pipeline/modules/braille/xml-to-pef.xpl" uri="../xml/xml-to-pef.xpl"  px:extends="http://www.daisy.org/pipeline/modules/braille/xml-to-pef/xml-to-pef.xpl"/>
  
  <!-- scripts -->
  <uri name="http://www.nlb.no/pipeline/modules/braille/dtbook-to-pef.xpl" uri="../xml/dtbook-to-pef.xpl"
       px:content-type="script" px:id="nlb:dtbook-to-pef" px:extends="http://www.nlb.no/pipeline/modules/braille/xml-to-pef.xpl"/>
  <uri name="http://www.nlb.no/pipeline/modules/braille/epub3-to-pef.xpl" uri="../xml/epub3-to-pef.xpl"
       px:content-type="script" px:id="nlb:epub3-to-pef" px:extends="http://www.nlb.no/pipeline/modules/braille/xml-to-pef.xpl"/>
  <uri name="http://www.nlb.no/pipeline/modules/braille/html-to-pef.xpl" uri="../xml/html-to-pef.xpl"
       px:content-type="script" px:id="nlb:html-to-pef" px:extends="http://www.nlb.no/pipeline/modules/braille/xml-to-pef.xpl"/>

  <!-- utilities -->
  <uri name="http://www.nlb.no/pipeline/modules/braille/insert-boilerplate.xsl" uri="../xml/insert-boilerplate.xsl"/>
  <uri name="http://www.nlb.no/pipeline/modules/braille/pre-processing.xsl" uri="../xml/pre-processing.xsl"/>

  <!-- xproc library -->
  <uri name="http://www.nlb.no/pipeline/modules/braille/library.xpl" uri="../xml/library.xpl"/>
  
  <!-- css -->
  <uri name="http://www.nlb.no/pipeline/modules/braille/default.scss" uri="../css/nlb-default.scss"/>
  
  <!-- hyphen dictionaries-->
  <uri name="http://www.nlb.no/hyphen/" uri="../hyph/" px:includes="*.dic" px:content-type="libhyphen-tables" px:id="nlb-hyphen-tables"/>
  
  <!-- dependencies -->
  <nextCatalog catalog="org:daisy:pipeline:modules:common-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:file-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:fileset-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:mediatype-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:zip-utils"/>
  
</catalog>
